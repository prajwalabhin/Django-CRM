{
    "version": 2,
    "builds": [
        {
            "src": "dcrm/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dcrm/wsgi.py"
        }
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "dcrm.settings",
        "PYTHONUNBUFFERED": "1",
        "MYSQL_DATABASE_HOST": "your-database-host",
        "MYSQL_DATABASE_NAME": "your-database-name",
        "MYSQL_DATABASE_USER": "your-database-user",
        "MYSQL_DATABASE_PASSWORD": "your-database-password",
        "DJANGO_SECRET_KEY": "your-django-secret-key"
    },
    "files": {
        "static": {
            "src": "website/static/**/*",
            "dest": "/static"
        }
    }
}




